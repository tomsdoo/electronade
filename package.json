{
  "name": "electronade",
  "version": "1.0.0",
  "description": "",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "bin": {
    "electronade": "bin/electronade.js"
  },
  "scripts": {
    "build": "tsc --project tsconfig.cjs.json && tsc --project tsconfig.esm.json && tsc --project tsconfig.bin.json",
    "lintfix:document": "npm run lintfix:document:html && npm run lintfix:document:css && npm run lintfix:document:server",
    "lintfix:document:css": "prettier --write docs/css/**/*.css",
    "lintfix:document:html": "prettier --write docs/index.html",
    "lintfix:document:server": "prettier --write doc_server.ts",
    "lintfix:src": "prettier --write src/**/*.ts",
    "lintfix:test": "prettier --write test/**/*.ts",
    "lintfix": "npm run lintfix:src && npm run lintfix:document && npm run lintfix:test",
    "serve:doc": "ts-node doc_server",
    "test": "TS_NODE_COMPILER_OPTIONS='{\"module\":\"commonjs\"}' mocha -r ts-node/register \"test/**/*.ts\" --timeout 30000"
  },
  "keywords": [],
  "author": "tom",
  "license": "MIT",
  "devDependencies": {
    "@types/commander": "^2.12.2",
    "@types/koa": "^2.13.5",
    "@types/mocha": "^9.1.1",
    "@types/sinon": "^10.0.13",
    "@types/uuid": "^8.3.4",
    "electron": "^20.0.2",
    "husky": "^8.0.1",
    "koa": "^2.13.4",
    "koa-router": "^12.0.0",
    "koa-static-server": "^1.5.2",
    "mocha": "^10.0.0",
    "prettier": "^2.7.1",
    "sinon": "^14.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "commander": "^9.4.0",
    "uuid": "^8.3.2"
  }
}
